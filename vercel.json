{
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "https://capy-back.vercel.app/api/submit",
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
          "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Authorization"
        }
      }
    ]
  }
  